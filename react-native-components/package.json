{
  "name": "whimsical-react-native",
  "version": "0.0.2",
  "keywords": [
    "react",
    "native",
    "whimsical",
    "components"
  ],
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "dev": "expo start -c",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build": "tsc --project tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.tsx\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn test && yarn lint",
    "version": "yarn test && yarn format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:ci": "yarn test --coverage",
    "test:badges": "yarn test:ci  && jest-coverage-badges --input coverage/coverage-summary.json --output __badges__"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.3",
    "expo": "~45.0.0",
    "expo-status-bar": "~1.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-reanimated": "~2.8.0",
    "react-native-ui-lib": "^6.15.0",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-typescript": "^7.16.7",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@types/jest": "^27.5.1",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "@types/react-test-renderer": "^18.0.0",
    "babel-loader": "^8.2.5",
    "jest": "^28.1.0",
    "jest-coverage-badges": "^1.1.2",
    "jest-environment-jsdom": "^28.1.0",
    "jest-expo": "^45.0.1",
    "metro-babel-register": "^0.70.3",
    "react-test-renderer": "^18.1.0",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "~4.3.5"
  },
  "private": false,
  "author": "Ty Barho",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/take2mfg/whimsical.git"
  },
  "files": [
    "lib/**/*"
  ],
  "license": "MIT"
}
